{
  "manifest_version": 3,
  "name": "elm-chrome-extension example",
  "version": "0.0.0",
  "permissions": [],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "elm-chrome-extension example",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ]
}
